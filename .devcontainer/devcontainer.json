{
	"name": "tobcalc",
	"image": "mcr.microsoft.com/devcontainers/typescript-node",
	"postCreateCommand": "npm install",
	"postAttachCommand": {
		"npm": "npm run dev"
	},
	"features": {
		"ghcr.io/devcontainers-contrib/features/deno": {}
	},
	"forwardPorts": [5173],
	"portsAttributes": {
		"5173": {
			"label": "Vite web app"
		}
	},
	"customizations": {
		"codespaces": {
			"openFiles": [
				"README.md"
			]
		},
		"vscode": {
			"extensions": [
				"svelte.svelte-vscode"
			]
		}
	}
}
